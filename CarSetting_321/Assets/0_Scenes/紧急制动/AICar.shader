			o.Albedo = ( tex2D( _MainTexture, uv_MainTexture ) * _AlbedoColor ).rgb;
			o.Smoothness = _Smoothness;
			float2 uv_AOTexture = i.uv_texcoord * _AOTexture_ST.xy + _AOTexture_ST.zw;
			o.Occlusion = tex2D( _AOTexture, uv_AOTexture ).r;
			o.Alpha = 0.85;
		}

